<h1>Dashboard</h1>

<h2>Next lesson</h2>
<p><%= @next_lesson.name %></p>
<p><%= @next_lesson.concept%></p>

<h2>Want to Refresh Your Memory?</h2>
<% @completed_lessons.each do |user_lesson| %>
<div class="card-lesson">
<%= image_tag('seed/Arrays.png', alt: 'Arrays') %>
  <h2><%= user_lesson.lesson.name %></h2>
  <p><%= user_lesson.lesson.description %></p>
</div>
<% end %>

<h2>Levels</h2>
<% @levels.each do | level| %>
<p><%= level.completed%></p>
<p><%= level.lesson.name %></p>
<% end %>
